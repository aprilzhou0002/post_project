
      <h2>Welcome, <%= user?.uname || "Visitor" %></h2>
      <div>
        <p>All Posts</p>
        <hr>
        <hr>
        <% posts.forEach(post => { %>
        <div class="individualPost">
          <a href="/posts/show/<%=post.id%>"><%= post.title %></a>
          <p><%= post.description %><br>Posted by: <%= post.creator.uname %></p>
          <hr>
        </div>
        <% }) %>
      </div>
      <hr>
      
      <br>
      
      
    
